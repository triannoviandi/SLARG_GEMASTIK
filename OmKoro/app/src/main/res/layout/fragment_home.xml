                        android:textColor="#fff"
                        android:textSize="15sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensans_bold"
                        android:text="Slarg"
                        android:textAlignment="textStart"
                        android:textColor="#fff"
                        android:textSize="15sp" />


