            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            app:cardUseCompatPadding="true"
            app:layout_constraintTop_toBottomOf="@+id/home_tvAtas">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_menu1"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:stateListAnimator="@animator/touch"
                    android:clickable="true"
                    android:focusable="true"
                    app:cardElevation="0dp"
                    android:foreground="?attr/selectableItemBackground"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/home_layoutMenu1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp"
                            >

                            <ImageView
                                android:id="@+id/home_imgFamily"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:src="@drawable/icon_family"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/opensans_regular"
                                android:text="Keluarga"
                                android:textSize="13sp" />

                        </LinearLayout>
                    </LinearLayout>



                </androidx.cardview.widget.CardView>
                <View
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="24dp"
